    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "prisma": "^5.7.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
